<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CATEREO" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/logotype_yellow</item>
        <item name="colorPrimaryVariant">@color/selective_yellow</item>
        <item name="colorOnPrimary">@color/dutch_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/carribbean_current</item>
        <item name="colorSecondaryVariant">@color/midnight_green</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="fontFamily">@font/lato</item>
        <!-- Customize your theme here. -->
    </style>
</resources>